<?xml version="1.0" encoding="UTF-8"?>
<scene>
    <duration>1000</duration>
    <title>
        <font-size>32</font-size>
        <name>Space</name>
        <color>#000044</color>
    </title>
    
    <array name="starColors" type="str">
        <value>#0000FF</value>
        <value>#0000AA</value>
        <value>#000077</value>
        <value>#000044</value>
    </array>
    
    <shapes>
        <loop>
            <i>0</i>
            <step>1</step>
            <count>
                <random>
                    <from>100</from>
                    <to>140</to>
                </random>
            </count>
            <loopContent>                <!-- equals a `shapes` tag -->
                <star>
                    <layer>2</layer>
                    <x>
                        <random>
                            <from>0</from>
                            <to>1280</to>
                        </random>
                    </x>
                    <y>
                        <random>
                            <from>0</from>
                            <to>640</to>
                        </random>
                    </y>
                    <w>
                        <random type="float">
                            <from>0.1</from>
                            <to>1</to>
                        </random>
                    </w>
                    <c1>
                        <getArray name="starColors">
                            <index>
                                <random>
                                    <from>0</from>
                                    <to>3</to>
                                </random>
                            </index>
                        </getArray>
                    </c1>
                    <c2>
                        <getArray name="starColors">
                            <index>
                                <random>
                                    <from>0</from>
                                    <to>3</to>
                                </random>
                            </index>
                        </getArray>
                    </c2>
                    <moveChain>
                        <node>
                            <endTime>90</endTime>
                            <move>no_move</move>
                        </node>
                        <node>
                            <endTime>90</endTime>
                            <move dx="-600" dy="0" timeUse="90">moveTo</move>
                        </node>
                        <node>
                            <endTime>350</endTime>
                            <move>no_move</move>
                        </node>
                        <node>
                            <endTime>90</endTime>
                            <move dx="600" dy="0" timeUse="90">moveTo</move>
                        </node>
                        <node>
                            <endTime>0</endTime>
                            <move></move>
                        </node>
                    </moveChain>
                </star>
            </loopContent>
        </loop>
        <ellipse>
            <layer>3</layer>
            <x>-121</x>
            <y>310</y>
            <w>242</w>
            <h>240</h>
            <color>#7fb0d7</color>
            <moveChain>
                <node>
                    <endTime>55</endTime>
                    <move></move>
                </node>
                <node>
                    <endTime>120</endTime>
                    <move dx="250" dy="0" timeUse="120">moveTo</move>
                </node>
                <node>
                    <endTime>350</endTime>
                    <move>no_move</move>
                </node>
                <node>
                    <endTime>120</endTime>
                    <move dx="-250" dy="0" timeUse="120">moveTo</move>
                </node>
                <node>
                    <endTime>0</endTime>
                    <move></move>
                </node>
            </moveChain>
            <zoomChain>
                <node>
                    <endTime>0</endTime>
                    <zoom timeUse="700">zoomToZero</zoom>
                </node>
            </zoomChain>
        </ellipse>
        <rect>
            <layer>4</layer>
            <rectMode>3</rectMode>
            <x>-30</x>
            <y>320</y>
            <w>50</w>
            <h>41</h>
            <color>#A8A8A8</color>
            <border>#999999</border>
            <moveChain>
                <node>
                    <endTime>65</endTime>
                    <move x0="0" y0="0" dx="2" dy="0">line_move</move>
                </node>
                <node>
                    <endTime>0</endTime>
                    <move>no_move</move>
                </node>
            </moveChain>
            <zoomChain>
                <node>
                    <endTime>90</endTime>
                    <zoom></zoom>
                </node>
                <node>
                    <endTime>120</endTime>
                    <zoom dw="-10" dh="0" timeUse="120">zoomTo</zoom>
                </node>
                <node>
                    <endTime>350</endTime>
                    <zoom></zoom>
                </node>
                <node>
                    <endTime>90</endTime>
                    <zoom dw="10" dh="0" timeUse="90">zoomTo</zoom>
                </node>
                <node>
                    <endTime>0</endTime>
                    <zoom></zoom>
                </node>
            </zoomChain>
        </rect>
    </shapes>
</scene>